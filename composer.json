{
  "name": "nunopress/doctrine-cache-service-provider",
  "description": "Doctrine Cache Service Provider for Silex Microframework or any other Pimple Container project's",
  "license": "MIT",
  "homepage": "http://nunopress.com",
  "support": {
    "issues": "https://github.com/nunopress/doctrine-cache-service-provider/issues",
    "source": "https://github.com/nunopress/doctrine-cache-service-provider"
  },
  "authors": [
    {
      "name": "SÃ©rgio Rafael Siqueira",
      "email": "sergio@inbep.com.br"
    },
    {
      "name": "NunoPress LLC",
      "email": "hello@nunopress.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "doctrine/cache": "^1.6",
    "pimple/pimple": "^3.0",
    "nunopress/doctrine-cache-pdo": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "pdepend/pdepend": "~2.0",
    "phing/phing": "~2.10",
    "phploc/phploc": "~2.1",
    "phpmd/phpmd": "~2.2",
    "sebastian/phpcpd": "~2.0",
    "squizlabs/php_codesniffer": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "NunoPress\\Silex\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NunoPress\\Silex\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.4-dev"
    }
  },
  "minimum-stability": "dev"
}
